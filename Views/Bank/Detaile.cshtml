@model MyDBProject.Models.Bank

@{
    ViewData["Title"] = "Detaile";
}

<h1>Bank Detail</h1>

<div>
   @* <h4>Bank</h4>*@
    <hr />
    <dl class="row">
        <dt class = "col-sm-2">
            Bank Name
        </dt>
        <dd class = "col-sm-10">
            @Html.DisplayFor(model => model.Bank_name)
        </dd>
        <dt class = "col-sm-2">
           Branch Name
        </dt>
        <dd class = "col-sm-10">
            @Html.DisplayFor(model => model.Branch_name)
        </dd>
        <dt class = "col-sm-2">
            Bank IFSC
        </dt>
        <dd class = "col-sm-10">
            @Html.DisplayFor(model => model.Bank_IFSC)
        </dd>
    </dl>
</div>
<div>
    <a class="btn btn-warning" asp-action="Edit" asp-route-id="@Model.Bank_id">Edit</a> |
    <a Class="btn btn-secondary" asp-action="Index">Back to List</a>
</div>
