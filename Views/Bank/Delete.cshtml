@model MyDBProject.Models.Bank

@{
    ViewData["Title"] = "Delete";
}

<h1>Delete Bank</h1>

<h3>Are you sure you want to delete this?</h3>
<div>
    @*<h4>Bank</h4>*@
    <hr />
    <dl class="row">
        <dt class = "col-sm-2">
            Bank Name
        </dt>
        <dd class = "col-sm-10">
            @Html.DisplayFor(model => model.Bank_name)
        </dd>
        <dt class = "col-sm-2">
            Branch Name
        </dt>
        <dd class = "col-sm-10">
            @Html.DisplayFor(model => model.Branch_name)
        </dd>
        <dt class = "col-sm-2">
            Bank IFSC
        </dt>
        <dd class = "col-sm-10">
            @Html.DisplayFor(model => model.Bank_IFSC)
        </dd>
    </dl>
    
    <form asp-action="Deleted">
        <input type="hidden" asp-for="Bank_id" />
        <input type="submit" value="Delete" class="btn btn-danger" /> | <a class="btn btn-secondary" asp-action="Index">Back to List</a>
    </form>
</div>
